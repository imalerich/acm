import java.util.Scanner;

public class Bridges {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		while (proc(s.nextLine())) { }
		s.close();
	}
	
	public static boolean proc(String line) {
		Pair p = new Pair(line);
		if (shouldTerminate(p)) {
			return false;
		}
		
		// TODO
		
		return true;
	}
	
	public static boolean shouldTerminate(Pair p) {
		return p.x == 0 && p.y == 0;
	}
}

class Pair {
	int x;
	int y;
	
	Pair(int X, int Y) {
		x = X;
		y = Y;
	}
	
	Pair(String line) {
		Scanner s = new Scanner(line);
		x = s.nextInt();
		y = s.nextInt();
		s.close();
	}
}