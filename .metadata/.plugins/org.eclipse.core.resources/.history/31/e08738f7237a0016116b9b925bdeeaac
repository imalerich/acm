import java.util.ArrayList;
import java.util.Scanner;

public class Tribute {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int count = s.nextInt();
		for (int i=0; i<count; i++) {
			proc(s.nextLine());
		}
	}
	
	public static void proc(String line) {
		ArrayList<Character> buffer = new ArrayList<Character>();
		int cursor = 0;
		
		// Process each command input by the user.
		for (int i=0; i<line.length(); i++) {
			char c = line.charAt(i);
			if (isCommand(c)) {
				// TODO
			} else {
				buffer.add(cursor++, c);
			}
		}
		
		// Set the current location of the cursor
		buffer.add(cursor, '^');
		StringBuilder b = new StringBuilder(buffer.size());
		for (Character c : buffer) { b.append(c); };
		System.out.println(b.toString());
	}
	
	public static boolean isCommand(char c) {
		switch (c) {
		case 'x':
		case 'K':
		case 'c':
		case 'D':
		case 'R':
		case 'p':
		case 'W':
		case 'h':
		case 'L':
		case 'f':
		case 'G':
			return true;
		default:
			return false;
		}
	}
}
