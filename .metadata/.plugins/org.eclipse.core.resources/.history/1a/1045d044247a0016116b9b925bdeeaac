import java.util.ArrayList;
import java.util.Scanner;

public class Tribute {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int count = s.nextInt();
		s.hasNextLine();
		for (int i=0; i<count; i++) {
			proc(s.nextLine());
		}
	}
	
	public static void proc(String line) {
		ArrayList<Character> buffer = new ArrayList<Character>();
		Integer cursor = 0;
		
		// Process each command input by the user.
		for (int i=0; i<line.length(); i++) {
			procCommand(line.charAt(i), buffer, cursor);
		}
		
		// Set the current location of the cursor
		buffer.add(cursor, '^');
		
		// Convert the buffer to a string for output.
		StringBuilder b = new StringBuilder(buffer.size());
		for (Character c : buffer) { b.append(c); };
		System.out.println(b.toString());
	}
	
	public static void procCommand(char cmd, ArrayList<Character> buffer, Integer cursor) {
		switch (cmd) {
		case 'x':
			break;
		case 'K':
			break;
		case 'c':
			break;
		case 'D':
			break;
		case 'R':
			break;
		case 'p':
			break;
		case 'W':
			break;
		case 'h':
			break;
		case 'L':
			break;
		case 'f':
			break;
		case 'G':
			break;
		default:
			buffer.add(cursor++, cmd);
			break;
		}
	}
}
