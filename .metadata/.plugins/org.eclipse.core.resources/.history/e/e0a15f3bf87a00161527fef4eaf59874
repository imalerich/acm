import java.util.Scanner;

public class Bridges {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
	}
}

class Pair {
	int x;
	int y;
	
	Pair(int X, int Y) {
		x = X;
		y = Y;
	}
	
	Pair(String line) {
		Scanner s = new Scanner(line);
		x = s.nextInt();
		y = s.nextInt();
	}
}